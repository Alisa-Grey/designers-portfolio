{
  "name": "artists-portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://Alisa-Grey.github.io/designers-portfolio",
  "dependencies": {
    "@chakra-ui/alert": "^2.0.11",
    "@types/node": "^16.11.62",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "fetch": "^1.1.0",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-obfuscate": "^3.6.8",
    "react-redux": "^8.0.4",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "validator": "^13.7.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:lint": "eslint --debug 'src/**/*.{js,jsx,ts,tsx}' --max-warnings=0 --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": [
            "error"
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "redux-devtools-extension": "^2.13.9"
  }
}
